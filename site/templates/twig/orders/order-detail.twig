{% set details = orderdisplay.get_orderdetails(order) %}
{% for detail in details %}
    <tr>
        <td>
            {{ detail.itemid }}</br>
            <small>{{ detail.desc1 }}</small>
        </td>
        <td class="text-right">{{ detail.qty }}</td>
        <td class="text-right">$ {{ detail.price }}</td>
        <td class="text-right">$ {{ detail.totalprice }}</td>
    </tr>
{% endfor %}
