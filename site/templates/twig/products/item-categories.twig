<div class="form-group">
    <h1 class="text-danger font-weight-bold border-bottom border-primary mt-4">{{ page.title }}</h1>
</div>
{% include 'products/search.twig' %}
<ul class="product-list">
    {% for item_category in item_categories %}
        <a href="{{ item_category.url }}">
            <li>{{ item_category.title|raw }}</li>
        </a>
    {% endfor %}
</ul>
